[alias]
  lol = log --graph --decorate --pretty=oneline --abbrev-commit --all --max-count=10
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  loldiff = log --graph --decorate --pretty=oneline --abbrev-commit HEAD ^master
  df = diff HEAD master
  ls = ls-files
  br = branch
  co = checkout
  ign = ls-files -o -i --exclude-standard
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

